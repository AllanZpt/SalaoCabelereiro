INSERT INTO [dbo].[Clientes]
           ([Name]
           ,[Aniversario]
           ,[CPF]
           ,[Email]
           ,[Celular]
           ,[DtUsualt]
           ,[DtUsuinc]
           ,[Perfil]
           ,[Senha])
     VALUES
           ('Admin'  -- Nome
           ,'1998-11-02 00:00:00'  -- Aniversário
           ,'12345678971'  -- CPF
           ,'admin@teste.com'  -- Email
           ,'1111111111'  -- Celular
           ,NULL  -- DtUsualt (se for NULL)
           ,SYSDATETIME()  -- DtUsuinc (data do sistema)
           ,2  -- Perfil (assumindo que é um valor numérico)
           ,'123')  -- Senha


